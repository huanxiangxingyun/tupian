<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <head>
    <title>简单OPML示例</title>
  </head>
  <body>
    <outline text="我的RSS订阅" title="我的RSS订阅">
      <outline text="OpenAI Blog" title="OpenAI Blog" type="rss" xmlUrl="https://openai.com/blog/rss.xml" htmlUrl="https://openai.com/blog/"/>
      <outline text="BBC News" title="BBC News" type="rss" xmlUrl="http://feeds.bbci.co.uk/news/rss.xml" htmlUrl="https://www.bbc.com/news"/>
    </outline>
  </body>
</opml>
